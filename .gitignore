# CCTV Analysis Project .gitignore

# Video files
*.mp4
*.avi
*.mov
*.mkv
*.wmv
*.flv
*.webm
*.m4v
*.mpg
*.mpeg
*.ts

# Highlight directories (but keep the directory structure)
highlights/*
!highlights/.gitkeep

# Output directories (but keep the directory structure)
output/*
!output/.gitkeep

# Python virtual environment
venv/
env/
ENV/
.env/
.venv/

# Python cache files
__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Distribution / packaging
dist/
build/
*.egg-info/
*.egg

# Log files
*.log
logs/
log/

# Temporary files
tmp/
temp/
*.tmp

# Model files (often large)
*.pt
*.pth
*.weights
*.onnx
*.tflite
*.h5

# Data files (often large or sensitive)
data/*
!data/.gitkeep
*.csv
*.npy
*.npz

# IDE and editor specific files
.idea/
.vscode/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Jupyter Notebook
.ipynb_checkpoints

# JSON output files
highlights/*.json
output/*.json

# FFmpeg specific
filelist.txt

# Windows specific
desktop.ini
$RECYCLE.BIN/

# Large test files
test*.mp4
